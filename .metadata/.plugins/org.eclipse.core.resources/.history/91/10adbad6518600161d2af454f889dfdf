package com.ourchars;

import com.interfaces.Warrior;
import com.parents.Halfling;

public class HalflingWarrior extends Halfling implements Warrior{

	public HalflingWarrior() {
		super();
		// TODO Auto-generated constructor stub
	}

	public HalflingWarrior(int health, int mana, String name) {
		super(health, mana, name);
		// TODO Auto-generated constructor stub
	}

	@Override
	public void changeHealth() {
		// TODO Auto-generated method stub
		Warrior.super.changeHealth();
	}

	@Override
	public void changeMana() {
		// TODO Auto-generated method stub
		Warrior.super.changeMana();
	}

	@Override
	public void weaponType() {
		// TODO Auto-generated method stub
		Warrior.super.weaponType();
	}
	
	

}
